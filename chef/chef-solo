#!/usr/bin/ruby
#
# This file was generated by RubyGems.
#
# The application 'chef' is installed as part of a gem, and
# this file is here to facilitate running it.
#

require 'rubygems'
require 'ruby-prof'
result = RubyProf.profile do
  version = ">= 0"

  if ARGV.first =~ /^_(.*)_$/ and Gem::Version.correct? $1 then
    version = $1
    ARGV.shift
  end

  gem 'chef', version
  load Gem.bin_path('chef', 'chef-solo', version)
end
printer = RubyProf::FlatPrinter.new(result)
output = File.open(File.join(File.dirname(__FILE__), "..", "chef-timing.prof"), "w")
printer.print(output, 0)
output.close

